<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Regression Tests" parallel="tests">
    <listeners>
        <listener class-name="com.sq.listeners.TestListener"/>
    </listeners>

    <test name="Client Test on Chrome">
        <parameter name="browserType" value="chrome"/>
        <classes>
            <class name="com.sq.tests.PageTest"/>
        </classes>
    </test>
    <test name="Client Test on Edge" enabled="false">
        <parameter name="browserType" value="edge"/>
        <classes>
            <class name="com.sq.tests.PageTest"/>
        </classes>
    </test>
</suite>
